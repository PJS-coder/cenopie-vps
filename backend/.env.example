# Development Environment Variables
NODE_ENV=development
PORT=4000

# Database
MONGODB_URI=mongodb://localhost:27017/cenopie

# JWT Secrets (generate your own for production)
JWT_SECRET=your_jwt_secret_here_change_this_in_production
JWT_REFRESH_SECRET=your_jwt_refresh_secret_here_change_this_in_production

# Cloudinary (for file uploads) - Get from https://cloudinary.com
CLOUDINARY_CLOUD_NAME=your_cloudinary_name
CLOUDINARY_API_KEY=your_cloudinary_key
CLOUDINARY_API_SECRET=your_cloudinary_secret

# Application URLs
CLIENT_ORIGIN=http://localhost:3000
FRONTEND_URL=http://localhost:3000

# Redis (optional for development)
REDIS_DISABLED=true

# Launch Control
ALLOW_REGISTRATION=true
LAUNCH_MODE=open

# Email Service (optional)
# EMAIL_SERVICE=gmail
# EMAIL_USER=your-email@gmail.com
# EMAIL_PASSWORD=your-app-password
# EMAIL_FROM=Cenopie <noreply@cenopie.com>